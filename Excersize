// Guest Name Start
var guestName=prompt("Guest Name : ");
var guestList=["Angela","Jack","Pam","James","Lara","Jason"];

if(guestList.includes(guestName)){
    alert( "perfect you are here");
}else{
   alert(  "Sorry, next time");
}
// Guest Name End





// Fizz Buzz Game Start
var output=[];
var count= 1 ;
 
function fizzBuzz() {

if(count % 3 === 0 && count%5===0){
     output.push("Fizz Buzz"); 
}
else if(count % 3 === 0) {
    output.push("fizz"); 
} 
else if(count % 5 === 0) {
    output.push("Buzz"); 
}
else{
    output.push(count);
}
 count++;
    console.log(output);
}
// Fizz Buzz Game End



// Who is Paying Lunch Today Start
var names=["Angela","Ben","Jenny","Michael","Chole"];

function whosPaying(names){
    
    var numberOfPeople=names.lenght;
    var randomPeronPosition=Math.floor(Math.random() * numberOfPeople);
    var randomPerson=names[randomPeronPosition];

    return randomPerson+" is going to buy lunch today";
}
// Who is Paying Lunch Today End


// Fibonacci Start

function fibonacciGenerator(n){
  var output=[];

  if(n===1){
    output=[0];
  }else if(n===2){
    output=[0,1];
  }else{
  output=[0, 1];
  
  for(var i=2; i<n; i++){
  output.push(output[output.lenght-2]+output[output.lenght-1]);
  }

  }
return output;
}
output=fibonacciGenerator(6);
  console.log(output);

// Fibonacci End
